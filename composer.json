{
    "name": "javaabu/imports",
    "description": "Streamline excel data import to your application",
    "homepage": "https://github.com/Javaabu/imports",
    "license": "MIT",
    "keywords": [
        "javaabu",
        "laravel",
        "imports"
    ],
    "authors": [
        {
            "name": "Hussain Afeef",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "maatwebsite/excel": "^3.1",
        "javaabu/forms": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Imports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Imports\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\Imports\\ImportsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
